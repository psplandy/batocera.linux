+/*******************************************************************************
+*: g_mod
+*uint32 g_mod( __u32 dividend, __u32 divisor, __u32 *quot_p )
+*: nand flashRAM
+*          OKERROR
+*: dividend          
+*          divisor           
+*          quot_p            
+*  : 
+*******************************************************************************/
+__u32 g_mod( __u32 dividend, __u32 divisor, __u32 *quot_p )
+{
+	if( divisor == 0 )
+	{
+		*quot_p = 0;
+		return 0;
+	}
+	if( divisor == 1 )
+	{
+		*quot_p = dividend;
+		return 0;
+	}
+
+	for( *quot_p = 0; dividend >= divisor; ++(*quot_p) )
+		dividend -= divisor;
+	return dividend;
+}
