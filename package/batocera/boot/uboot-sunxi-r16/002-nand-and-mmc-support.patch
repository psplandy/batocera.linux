+extern char boot0_hash_value[64];
+static void print_commit_log(void)
+{
+        printf("boot0 commit : %s \n",boot0_hash_value);
+
+        return ;
+}
+/*
+************************************************************************************************************
+*
+*                                             function
+*
+*    name          :
+*
+*    parmeters     :
+*
+*    return        :
+*
+*    note          :
+*
+*
+************************************************************************************************************
+*/
+static int boot0_clear_env(void)
+{
+
+	reset_pll();
+	mmu_turn_off();
+
+	__msdelay(10);
+
+	return 0;
+}
+
+
