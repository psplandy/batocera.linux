+*          CHECK_IS_WRONG        
+*    :
+********************************************************************************/
+__s32 check_magic( __u32 *mem_base, const char *magic )
+{
+	struct spare_boot_head_t *bfh;
+
+	bfh = (struct spare_boot_head_t *)mem_base;
+	if(!(strncmp((const char *)bfh->boot_head.magic, magic, 8)))
+	{
+		return 0;
+	}
+
+	return -1;
+}
+
+//#pragma arm section
+
+
+
+
+//#pragma arm section  code="check_sum"
+/********************************************************************************
+*: check_sum
+*: __s32 check_sum( __u32 *mem_base, __u32 size, const char *magic )
+*: 
+*: mem_base           4
+*          size               4
+*  : CHECK_IS_CORRECT   
+*          CHECK_IS_WRONG     
+*    :
+********************************************************************************/
+__s32 check_sum( __u32 *mem_base, __u32 size )
+{
+	__u32 *buf;
+	__u32 count;
+	__u32 src_sum;
+	__u32 sum;
+	struct spare_boot_head_t  *bfh;
+
+
+	bfh = (struct spare_boot_head_t *)mem_base;
+
+	/*  */
+	src_sum = bfh->boot_head.check_sum;                  // Boot_file_headcheck_sum
+	bfh->boot_head.check_sum = STAMP_VALUE;              // STAMP_VALUEBoot_file_headcheck_sum
+
+	count = size >> 2;                         //  4bytes
+	sum = 0;
+	buf = (__u32 *)mem_base;
+	do
+	{
+		sum += *buf++;                         // 
+		sum += *buf++;                         // 
+		sum += *buf++;                         // 
+		sum += *buf++;                         // 
+	}while( ( count -= 4 ) > (4-1) );
+
+	while( count-- > 0 )
+		sum += *buf++;
+
+	bfh->boot_head.check_sum = src_sum;                  // Boot_file_headcheck_sum
+
+	printf("sum=%x\n", sum);
+	printf("src_sum=%x\n", src_sum);
+
+	if( sum == src_sum )
+		return 0;               // 
+	else
+		return -1;                 // 
+}
+
+//#pragma arm section
+
+
+
+//#pragma arm section  code="check_file"
+/********************************************************************************
+*: check_file
+*: __s32 check_file( __u32 *mem_base, __u32 size, const char *magic )
+*: 
+*: mem_base       4
+*          size           4
+*          magic          magic number, 
+*  : CHECK_IS_CORRECT       
+*          CHECK_IS_WRONG         
+*    :
+********************************************************************************/
+__s32 check_file( __u32 *mem_base, __u32 size, const char *magic )
+{
+	if( check_magic( mem_base, magic ) == 0
+        &&check_sum( mem_base, size  ) == 0 )
+        return 0;
+    else
+    	return -1;
+}
+
+
+int verify_addsum( void *mem_base, __u32 size )
+{
