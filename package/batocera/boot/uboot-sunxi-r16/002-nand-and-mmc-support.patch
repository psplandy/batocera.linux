+int verify_addsum( void *mem_base, __u32 size )
+{
+	__u32 *buf;
+	__u32 count;
+	__u32 src_sum;
+	__u32 sum;
+	sbrom_toc1_head_info_t  *bfh;
+
+	bfh = (sbrom_toc1_head_info_t *)mem_base;
+
+	/*  */
+	src_sum = bfh->add_sum;                  // Boot_file_headcheck_sum
+	bfh->add_sum = STAMP_VALUE;              // STAMP_VALUEBoot_file_headcheck_sum
+
+	count = size >> 2;                         //  4bytes
+	sum = 0;
+	buf = (__u32 *)mem_base;
+	do
+	{
+		sum += *buf++;                         // 
+		sum += *buf++;                         // 
+		sum += *buf++;                         // 
+		sum += *buf++;                         // 
+	}while( ( count -= 4 ) > (4-1) );
+
+	while( count-- > 0 )
+		sum += *buf++;
+
+	bfh->add_sum = src_sum;                  // Boot_file_headcheck_sum
+
+	if( sum == src_sum )
+		return 0;                           // 
+	else
+		return -1;                          // 
+}
